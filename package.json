    "@babel/register": "^7.18.9",
    "@reduxjs/toolkit": "^1.8.6",
    "axios": "^1.1.3",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "css-loader": "^6.8.1",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^13.3.2",
