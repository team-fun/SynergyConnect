{
  "name": "synergyconnect",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "main": "index.js",
  "scripts": {
    "build:css": "npx tailwindcss build tailwind.css -o public/styles/style.css --watch",
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server & npm run build:css --watch",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "server": "node socket/index.js",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "autoprefixer": "^10.4.14",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.37.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "perfect-freehand": "^1.2.0",
    "pg": "^8.8.0",
    "postcss": "^8.4.24",
    "react-big-calendar": "^1.8.1",
    "react-datepicker": "^4.15.0",
    "react-icons": "^4.10.1",
    "react-jitsi": "^1.0.4",
    "react-modal": "^3.16.1",
    "react-scroll-to-bottom": "^4.2.0",
    "roughjs": "^4.5.2",
